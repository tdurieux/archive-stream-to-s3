    "lint": "./node_modules/.bin/tslint -p tsconfig.json"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "tdurieux/archive-stream-to-s3.git",
  "engines": {
    "node": ">=8.10.0"
  },
  "keywords": [
