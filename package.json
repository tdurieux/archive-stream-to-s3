  "author": "public@durieux.me",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tdurieux/archive-stream-to-s3/issues"
  },
  "homepage": "https://github.com/tdurieux/archive-stream-to-s3#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.1",
    "@types/mime-types": "^2.1.1",
