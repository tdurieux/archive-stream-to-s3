    "@types/tar-stream": "^2.2.2",
    "@types/unzip-stream": "^0.3.1",
    "aws-sdk-client-mock": "^2.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "jest": "^29.5.0",
    "release-it": "^15.10.2",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4",
    "jszip": "^3.10.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
